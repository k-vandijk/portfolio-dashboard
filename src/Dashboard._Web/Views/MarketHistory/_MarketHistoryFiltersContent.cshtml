@model MarketHistoryFiltersContentViewModel

<label class="filter-label mb-1">@Model.SharedLocalizer["Ticker"]</label>
<div class="btn-group w-100" role="group">
    @foreach (var t in Model.Tickers)
    {
        <a href="@($"{Model.SystemUrlMarketHistory}?ticker={t}")"
           class="btn btn-ticker px-3 py-2 @(t == Model.CurrentTicker ? "active" : "")">
            @t
        </a>
    }
</div>